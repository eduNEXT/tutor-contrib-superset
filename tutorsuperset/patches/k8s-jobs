---
apiVersion: batch/v1
kind: Job
metadata:
  name: superset-job
  labels:
    app.kubernetes.io/component: job
spec:
  template:
    spec:
      restartPolicy: Never
      containers:
      - name: superset
        image: apache/superset:{{ SUPERSET_TAG }}

---
apiVersion: batch/v1
kind: Job
metadata:
  name: superset-worker-jon
  labels:
    app.kubernetes.io/component: job
spec:
  template:
    spec:
      restartPolicy: Never
      containers:
      - name: superset-worker
        image: apache/superset:{{ SUPERSET_TAG }}
